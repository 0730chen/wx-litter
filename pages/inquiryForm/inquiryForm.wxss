/* pages/inquiryForm/inquiryForm.wxss */
.question-title{
  background: #f5f5f5;
  padding: 8px 0;
}
.question{
  box-sizing: border-box;
}
.inquiry-wrap{
  width: 100%;
  height: 100%;
  overflow: hidden;
  display: flex;
  flex-direction: column;
}
.question-container{
  display: flex;
  flex-direction: column;
  padding: 0 12px;
  background: white;
  flex: 1;
  overflow: auto;
}
.question-wrap{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  flex-wrap: wrap;
}
.submit{
  padding: 8px;
  text-align: center;
}
/* pages/inquiryForm/inquiryForm.wxss */
.question-title{
  background: #f5f5f5;
  padding: 16rpx;
  border-radius: 5px;
}
.question{
  box-sizing: border-box;
  padding: 8px;
}
.inquiry-wrap{
  width: 100%;
  height: 100%;
  overflow: hidden;
  display: flex;
  flex-direction: column;
}
.question-container{
  display: flex;
  flex-direction: column;
  background: white;
  flex: 1;
  overflow: auto;
}
.question-wrap{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  flex-wrap: wrap;
}
.van-col--12{
  padding: 8px;
}